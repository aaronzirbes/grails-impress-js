<div id="slide-${it.id}" 
	class="step slide"
	data-x="${it.positionX}" 
	data-y="${it.positionY}" 
	data-z="${it.positionZ}" 
	data-rotate-x="${it.rotationX}"
	data-rotate-y="${it.rotationY}"
	data-rotate-z="${it.rotationZ}"
	data-scale="${it.scale}">
	${it.content}
	<div class="slide-num">${it.sortOrder}</div>
</div>
